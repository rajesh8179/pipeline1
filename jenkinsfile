pipeline{
agent any
   stages{
stage('one'){
steps{
echo 'hi, this is rajeh'
}
}
stage('two')
{
steps{
input('do you want proceed?')
}
}
stage('three')
{
when{
  not{
branch "master"
}
}
steps{
    echo "hello"
}
}
stage('four')
{
parallel{
stage('unit test'){
steps{
       echo "running the unit test...."
}
}
stage('integration test'){
agent{
docker{
reuseNode false
 image 'ubuntu'
}
}
steps{
echo "running integration test.."
}
}
}
}
}
}
